{
    "title": "Hotkeys to switch app",
    "rules": [
        {
            "description": "right_command-, to System Preferences",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                            "mandatory": [
                                "right_command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'System Preferences'"
                        }
                    ]
                }
            ]
        },
